<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trevor.android.tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".EnterBooleanActivity"></activity>
        <activity android:name=".EnterDateActivity"></activity>
        <activity android:name=".EnterGastrointestinalActivity"></activity>
        <activity android:name=".EnterMenstrualActivity"></activity>
        <activity android:name=".EnterMiscActivity"></activity>
        <activity android:name=".EnterPainActivity"></activity>
        <activity android:name=".EnterScaleActivity"></activity>
        <activity android:name=".EnterSexualActivity"></activity>
        <activity android:name=".EnterSleepActivity"></activity>
        <activity android:name=".EnterTextActivity"></activity>
    </application>

</manifest>
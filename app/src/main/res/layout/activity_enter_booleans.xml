<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.trevor.android.tracker.DataEntryChooserActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/text_view_get_boolean_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="boolean_activity_false"
            android:textSize="30sp"
            android:visibility="invisible" />

        <Button
            android:id="@+id/text_view_get_boolean_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="boolean_activity_true"
            android:textSize="30sp"
            android:visibility="invisible" />
    </LinearLayout>

</LinearLayout>
